<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SetGameTimeController">
   <children>
      <Button fx:id="backbutton" layoutX="565.0" layoutY="534.0" mnemonicParsing="false" onAction="#backonclick" prefHeight="26.0" prefWidth="79.0" style="-fx-background-color: #0598ff;" text="Back" textFill="WHITE" />
      <Button fx:id="Submitbutton" layoutX="121.0" layoutY="402.0" mnemonicParsing="false" onAction="#submitonclick" prefHeight="26.0" prefWidth="72.0" text="Submit" />
      <Label layoutX="286.0" layoutY="14.0" prefHeight="30.0" prefWidth="150.0" text="Set Game Time">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="gamenametextfild" layoutX="95.0" layoutY="108.0" onMouseClicked="#clinegamenametextfild" promptText="Game Name" />
      <TextField fx:id="timeofgaetextfild" layoutX="95.0" layoutY="247.0" onMouseClicked="#clinetimetextfild" promptText="Time of game" />
      <TextField fx:id="groundtextfild" layoutX="95.0" layoutY="320.0" onMouseClicked="#clinegroundtextfild" promptText="Play Ground name" />
      <DatePicker fx:id="dateofgamepiker" layoutX="95.0" layoutY="176.0" onMouseClicked="#clinedateciker" promptText="Date of game" />
   </children>
</AnchorPane>
