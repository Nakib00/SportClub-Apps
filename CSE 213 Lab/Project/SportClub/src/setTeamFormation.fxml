<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SetTeamFormationController">
   <children>
      <Label layoutX="271.0" layoutY="14.0" text="Upload Team Formation">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="uploadbutton" layoutX="165.0" layoutY="300.0" mnemonicParsing="false" onAction="#uploadonclick" prefHeight="26.0" prefWidth="78.0" text="Upload" />
      <Button fx:id="backbutton" layoutX="586.0" layoutY="543.0" mnemonicParsing="false" onAction="#backonclick" prefHeight="26.0" prefWidth="77.0" style="-fx-background-color: #0598ff;" text="Back" textFill="WHITE" />
      <TextField fx:id="idtextfild" layoutX="94.0" layoutY="137.0" promptText="Player Id" />
      <TextField fx:id="gamenametextfild" layoutX="94.0" layoutY="198.0" promptText="Game Name" />
      <TextField fx:id="playerPositiontextfild" layoutX="94.0" layoutY="252.0" promptText="player Position" />
   </children>
</AnchorPane>
